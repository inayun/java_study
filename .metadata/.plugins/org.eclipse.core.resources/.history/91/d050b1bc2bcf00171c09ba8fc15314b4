package ex_first;

public class Ex_Test {

	public static void main(String[] args){
		
		// main 메소드 : 컴파일시에 가장 먼저 호출되는 영역
		
		System.out.println("hi");
		System.out.println("안뇽");
		System.out.println(20 + 20 + "500");
		System.out.println("500" + 20 + 20);
		System.out.println("500" + (20 + 20));

	}
	
	
}
